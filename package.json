{
  "name": "react-md-renderer",
  "version": "0.0.2-alpha.2",
  "description": "Simple batteries included rendering library for markdown files.",
  "keywords": [
    "react",
    "@mui/material",
    "@material-ui/core",
    "render"
  ],
  "repository": "agmoss/react-md-renderer",
  "license": "MIT",
  "author": "Andrew Moss",
  "sideEffects": false,
  "typesVersions": {
    "*": {
      "v4": [
        "src/v4/index.ts"
      ],
      "v5": [
        "src/v5/index.ts"
      ]
    }
  },
  "exports": {
    "./v4": "./src/v4/index.ts",
    "./v5": "./src/v5/index.ts"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "yarn run build"
  },
  "dependencies": {
    "markdown-to-jsx": "^7.1.7",
    "react-syntax-highlighter": "^15.5.0",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@material-ui/core": "latest",
    "@mui/material": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/react-syntax-highlighter": "latest",
    "typescript": "latest"
  },
  "peerDependencies": {
    "@material-ui/core": "*",
    "@mui/material": "*",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "@material-ui/core": {
      "optional": true
    },
    "@mui/material": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=10"
  }
}
